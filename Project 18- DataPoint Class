class DataPoint:
    def __init__(self, features: dict, label: str):
        self.features= features
        self.label= label
    def __repr__(self):
           return f"DataPoint(features={self.features}, label='{self.label}')"
p1 = DataPoint({'x': 1.5, 'y': 3.2}, 'Class_A')
p2 = DataPoint({'x': 0.9, 'y': 0.1}, 'Class_B')
print(f"Single Point: {p1}")

dataset = [p1, p2]
print(f"Dataset List: {dataset}")
